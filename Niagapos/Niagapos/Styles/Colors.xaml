    <SolidColorBrush x:Key="VisualDark2Brush" Color="{StaticResource VisualDark2Color}" />
    <SolidColorBrush x:Key="VisualDark3Brush" Color="{StaticResource VisualDark3Color}" />
    <SolidColorBrush x:Key="VisualDark4Brush" Color="{StaticResource VisualDark4Color}" />
    <SolidColorBrush x:Key="VisualDark5Brush" Color="{StaticResource VisualDark5Color}" />
    <SolidColorBrush x:Key="VisualDark6Brush" Color="{StaticResource VisualDark6Color}" />
    <SolidColorBrush x:Key="VisualDark7Brush" Color="{StaticResource VisualDark7Color}" />
    <SolidColorBrush x:Key="VisualDark8Brush" Color="{StaticResource VisualDark8Color}" />
    <SolidColorBrush x:Key="VisualDark9Brush" Color="{StaticResource VisualDark9Color}" />
    <SolidColorBrush x:Key="VisualDark10Brush" Color="{StaticResource VisualDark10Color}" />
    <!-- Visual Darkness -->
    <SolidColorBrush x:Key="VisualDarkness1Brush" Color="{StaticResource VisualDarkness1Color}" />
    <SolidColorBrush x:Key="VisualDarkness2Brush" Color="{StaticResource VisualDarkness2Color}" />
    <SolidColorBrush x:Key="VisualDarkness7Brush" Color="{StaticResource VisualDarkness7Color}" />
    <!-- Orange -->
    <SolidColorBrush x:Key="VisualFire1Brush" Color="{StaticResource VisualFire1Color}" />






    <!-- End Visual Dark Theme -->



    <!-- Nexus Section -->
    <Color x:Key="NexusPrimary">#4b367b</Color>
    <Color x:Key="SmoothPurpleNexus">#68217a</Color>
    
    <SolidColorBrush x:Key="NexusPrimaryBrush" Color="{StaticResource NexusPrimary}" />
    <SolidColorBrush x:Key="SmoothPurpleNexusBrush" Color="{StaticResource SmoothPurpleNexus}" />
    <!-- End Point -->

    <!-- Default Color Section -->
    <Color x:Key="DefaultBlue">#007bff</Color>
    <Color x:Key="DefaultRed">#dc3545</Color>
    <Color x:Key="DefaultYellow">#ffc107</Color>
    <Color x:Key="DefaultGreen">#28a745</Color>
    <Color x:Key="DefaultGray">#6c757d</Color>
    <Color x:Key="DefaultBlack">#343a40</Color>
    <Color x:Key="DefaultLight">#f8f9fa</Color>
    <Color x:Key="DefaultWhite">#ffffff</Color>
    <Color x:Key="DefaultTosca">#17a2b8</Color>
    <Color x:Key="DefaultWhiteMilk">#f9f9f9</Color>
    <Color x:Key="DefaultWhiteCream">#eeeeee</Color>

    <SolidColorBrush x:Key="DefaultBlueBrush" Color="{StaticResource DefaultBlue}" />
    <SolidColorBrush x:Key="DefaultRedBrush" Color="{StaticResource DefaultRed}" />
    <SolidColorBrush x:Key="DefaultYellowBrush" Color="{StaticResource DefaultYellow}" />
    <SolidColorBrush x:Key="DefaultBlackBrush" Color="{StaticResource DefaultBlack}" />
