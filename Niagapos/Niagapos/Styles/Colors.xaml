    <Color x:Key="VisualAzure1Color">#FF007ACC</Color>
    <Color x:Key="VisualAzure2Color">#FF3399FF</Color>
    <!-- Purple -->
    <Color x:Key="VisualAbbys1Color">#FF44355B</Color>
