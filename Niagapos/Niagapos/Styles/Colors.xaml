    <Color x:Key="TechNinetyDark">#2a3744</Color>
    <Color x:Key="TechNinetyTransparentDark">#002a3744</Color>
    <Color x:Key="TechNinetyDeep">#30373b</Color>
    <Color x:Key="TechNinetyGray">#454b4f</Color>

    <SolidColorBrush x:Key="TechNinetyPrimaryBrush" Color="{StaticResource TechNinetyPrimary}" />
