    <SolidColorBrush x:Key="TechNinetyTransparentDarkBrush" Color="{StaticResource TechNinetyTransparentDark}" />
    <SolidColorBrush x:Key="TechNinetyGrayBrush" Color="{StaticResource TechNinetyGray}" />
