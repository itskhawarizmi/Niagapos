    <SolidColorBrush x:Key="VisualLight1Brush" Color="{StaticResource VisualLight1Color}" />
    <SolidColorBrush x:Key="VisualLight2Brush" Color="{StaticResource VisualLight2Color}" />
    <SolidColorBrush x:Key="VisualLight3Brush" Color="{StaticResource VisualLight3Color}" />
    <SolidColorBrush x:Key="VisualLight4Brush" Color="{StaticResource VisualLight4Color}" />
    <SolidColorBrush x:Key="VisualLight5Brush" Color="{StaticResource VisualLight5Color}" />
    <SolidColorBrush x:Key="VisualLight6Brush" Color="{StaticResource VisualLight6Color}" />
    <SolidColorBrush x:Key="VisualLight7Brush" Color="{StaticResource VisualLight7Color}" />
    <SolidColorBrush x:Key="VisualLight8Brush" Color="{StaticResource VisualLight8Color}" />
