    <Color x:Key="TwitterColor">#29c5f6</Color>
    <Color x:Key="YouTubeColor">#cd201f</Color>
    <Color x:Key="InstagramColor">#4e729a</Color>
    <Color x:Key="GoogleColor">#cb2027</Color>
    <Color x:Key="InternetColor">#ffc200</Color>

    <SolidColorBrush x:Key="FacebookBrush" Color="{StaticResource FacebookColor}" />
    <SolidColorBrush x:Key="TwitterBrush" Color="{StaticResource TwitterColor}" />
    <SolidColorBrush x:Key="YouTubeBrush" Color="{StaticResource YouTubeColor}" />
    <SolidColorBrush x:Key="InstagramBrush" Color="{StaticResource InstagramColor}" />
    <SolidColorBrush x:Key="GoogleBrush" Color="{StaticResource GoogleColor}" />
    <SolidColorBrush x:Key="InternetBrush" Color="{StaticResource InternetColor}" />
