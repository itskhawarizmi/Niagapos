    <Color x:Key="VisualDark3Color">#FF3E3E40</Color>
    <Color x:Key="VisualDark4Color">#FF1B1B1C</Color>
    <Color x:Key="VisualDark5Color">#FF292929</Color>
    <Color x:Key="VisualDark6Color">#FF3F3F46</Color>
    <Color x:Key="VisualDark7Color">#FF333337</Color>
    <Color x:Key="VisualDark8Color">#FF656565</Color>
    <Color x:Key="VisualDark9Color">#72555555</Color>
    <Color x:Key="VisualDark10Color">#FF1E1E1E</Color>
    <!-- Orange -->
    <Color x:Key="VisualFire1Color">#FFF9BF3B</Color>
